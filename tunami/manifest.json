{
  "manifest_version": 2,
  "name": "Tunami",
  "version": "1",
  "minimum_chrome_version": "24",
  "app": {
    "background": {
      "scripts" : [
        "lib/underscore.min.js",
        "lib/Class.js",
        "lib/zip/zip.js",
        "lib/zip/zip-fs.js",
        "lib/zip/mime-types.js",
        "lib/zip/inflate.js",
        "lib/zip/deflate.js",
        "src/background.js",
        "src/utility.js",
        "src/Song.js"
      ]
    }
  },
  "permissions": [
    "fileSystem"
  ]
}

